<p style="color: green"><%= notice %></p>

<h1 class="text-white mx-3">Projects</h1>

<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
  <% end %>
</div>
  <div class="m-2">
    <% if user_signed_in? %>
      <%= link_to "New project", new_project_path if current_user.admin?%>
    <% end %>
  </div>